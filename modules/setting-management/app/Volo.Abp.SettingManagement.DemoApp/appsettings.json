{
  "AllowedHosts": "*",
  "AuthServer": {
    "Audience": "Demo",
    "Authority": "http://106.13.130.51",
    "IssuerUri": "http://106.13.130.51"
  },
  "ConnectionStrings": {
    "Default": "Server=106.75.247.11;Database=Abp.VNext.Hello;MultipleActiveResultSets=true;User ID=00;Password=007"
  },
  "Serilog": {
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "System.Net.Http.HttpClient": "Warning"
      }
    },
    "Properties": {
      "Application": "AspNetCoreSerilogDemo"
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Args": {
          "configureLogger": {
            "Filter": [
              {
                "Args": {
                  "expression": "Contains(SourceContext, 'Volo.Abp.SettingManagement.DemoApp') and (@Level = 'Error' or @Level = 'Fatal' or @Level = 'Warning')"
                },
                "Name": "ByIncludingOnly"
              }
            ],
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "outputTemplate": "{Timestamp:o} [{Level:u3}] ({SourceContext}) {Message}{NewLine}{Exception}",
                  "path": "Logs/Error/applog_.log",
                  "retainedFileCountLimit": 7,
                  "rollingInterval": "Day"
                }
              }
            ]
          }
        },
        "Name": "Logger"
      },
      {
        "Args": {
          "configureLogger": {
            "Filter": [
              {
                "Name": "ByIncludingOnly",
                "Args": {
                  "expression": "Contains(SourceContext, 'Volo.Abp.SettingManagement.DemoApp') and @Level = 'Information'"
                }
              }
            ],
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "outputTemplate": "{Timestamp:o} [{Level:u3}] ({SourceContext}) {Message}{NewLine}{Exception}",
                  "path": "Logs/Info/applog_.log",
                  "retainedFileCountLimit": 7,
                  "rollingInterval": "Day"
                }
              }
            ]
          }
        },
        "Name": "Logger"
      }
    ]
  }
}
